\n    def check_attendance(self):\n        \"\"\"출석 상태 확인\"\"\"\n        script = \"\"\"\n        var students = {};\n        \n        // 모든 학생 카드 찾기 (등원/하원 텍스트가 있는 요소)\n        document.querySelectorAll('*').forEach(function(elem) {\n            var text = elem.innerText || '';\n            \n            // 등원/하원 텍스트가 있고, 학생 이름이 포함된 카드\n            if ((text.includes('등원') || text.includes('하원')) && text.length < 200) {\n                // 한글 이름 추출 (2-4글자)\n                var nameMatch = text.match(/[가-힣]{2,4}/);\n                \n                if (nameMatch) {\n                    var studentName = nameMatch[0];\n                    \n                    // 시스템 메뉴 제외\n                    var systemWords = ['등원', '하원', '출결', '수납', '전체', '선택', \n                                      '납부', '일괄', '정보', '수정', '미납', '처리'];\n                    var isSystem = false;\n                    for (var word of systemWords) {\n                        if (studentName === word) {\n                            isSystem = true;\n                            break;\n                        }\n                    }\n                    \n                    if (!isSystem) {\n                        // 배경색 확인 (학생 카드 또는 부모 요소)\n                        var bgColor = window.getComputedStyle(elem).backgroundColor;\n                        var parent = elem.parentElement;\n                        if (parent) {\n                            var parentBg = window.getComputedStyle(parent).backgroundColor;\n                            if (parentBg && parentBg !== 'rgba(0, 0, 0, 0)') {\n                                bgColor = parentBg;\n                            }\n                        }\n                        \n                        // 스카이블루 확인\n                        var isSkyBlue = false;\n                        if (bgColor && bgColor !== 'rgba(0, 0, 0, 0)') {\n                            var rgbMatch = bgColor.match(/rgb\\\\((\\\\d+),\\\\s*(\\\\d+),\\\\s*(\\\\d+)/);\n                            if (rgbMatch) {\n                                var r = parseInt(rgbMatch[1]);\n                                var g = parseInt(rgbMatch[2]);\n                                var b = parseInt(rgbMatch[3]);\n                                \n                                // 스카이블루 범위\n                                if (r >= 100 && r <= 150 && g >= 180 && g <= 220 && b >= 200 && b <= 250) {\n                                    isSkyBlue = true;\n                                }\n                            }\n                        }\n                        \n                        // 시간 추출\n                        var timeMatch = text.match(/(\\\\d{1,2}:\\\\d{2})/);\n                        var checkinTime = timeMatch ? timeMatch[0] : null;\n                        \n                        students[studentName] = {\n                            isCheckedIn: isSkyBlue,\n                            checkinTime: checkinTime,\n                            bgColor: bgColor\n                        };\n                    }\n                }\n            }\n        });\n        \n        return students;\n        \"\"\"\n        \n        return self.driver.execute_script(script)\n        \n    def monitor(self):\n        \"\"\"실시간 모니터링\"\"\"\n        logger.info(\"=\" * 50)\n        logger.info(\"실시간 출결 모니터링 시작\")\n        logger.info(\"새로운 출석이 감지되면 알림이 표시됩니다\")\n        logger.info(\"종료: Ctrl+C\")\n        logger.info(\"=\" * 50)\n        \n        check_count = 0\n        \n        while True:\n            try:\n                check_count += 1\n                current_students = self.check_attendance()\n                \n                # 첫 체크 또는 변화 감지\n                if check_count == 1:\n                    self.students = current_students\n                    total = len(current_students)\n                    checked_in = len([s for s in current_students.values() if s['isCheckedIn']])\n                    logger.info(f\"\\n초기 상태: {checked_in}/{total}명 출석\")\n                    \n                    if checked_in > 0:\n                        logger.info(\"현재 출석한 학생:\")\n                        for name, info in current_students.items():\n                            if info['isCheckedIn']:\n                                time_str = f\" ({info['checkinTime']})\" if info['checkinTime'] else \"\"\n                                logger.info(f\"  ✅ {name}{time_str}\")\n                else:\n                    # 변화 감지\n                    for name, info in current_students.items():\n                        if name in self.students:\n                            old_status = self.students[name]['isCheckedIn']\n                            new_status = info['isCheckedIn']\n                            \n                            if not old_status and new_status:\n                                # 새로운 출석!\n                                time_str = f\" ({info['checkinTime']})\" if info['checkinTime'] else \"\"\n                                logger.info(f\"\\n🔔 새로운 출석: {name}{time_str}\")\n                                \n                                # Windows 알림음\n                                try:\n                                    winsound.Beep(1000, 200)  # 1000Hz, 200ms\n                                except:\n                                    pass\n                                    \n                            elif old_status and not new_status:\n                                # 출석 취소 (거의 없는 경우)\n                                logger.info(f\"\\n⚪ 출석 취소: {name}\")\n                    \n                    self.students = current_students\n                \n                # 30초마다 요약 출력\n                if check_count % 6 == 0:  # 5초 * 6 = 30초\n                    total = len(current_students)\n                    checked_in = len([s for s in current_students.values() if s['isCheckedIn']])\n                    print(f\"\\r[{datetime.now().strftime('%H:%M:%S')}] 출석 현황: {checked_in}/{total}명\", end=\"\")\n                \n                time.sleep(5)  # 5초마다 체크\n                \n            except KeyboardInterrupt:\n                logger.info(\"\\n모니터링 종료\")\n                break\n            except Exception as e:\n                logger.error(f\"오류 발생: {e}\")\n                time.sleep(5)\n                \n    def close(self):\n        \"\"\"브라우저 종료\"\"\"\n        if self.driver:\n            self.driver.quit()\n            logger.info(\"브라우저 종료\")\n\nif __name__ == \"__main__\":\n    monitor = AttokRealTimeMonitor()\n    try:\n        monitor.login()\n        monitor.monitor()\n    finally:\n        monitor.close()","structuredPatch":[]}}
