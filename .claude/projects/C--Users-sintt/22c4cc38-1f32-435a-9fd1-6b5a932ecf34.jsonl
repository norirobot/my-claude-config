Interrupt:\n                logger.info(\"\\n모니터링 종료\")\n                break\n            except Exception as e:\n                logger.error(f\"오류 발생: {e}\")\n                time.sleep(5)\n                \n    def close(self):\n        \"\"\"브라우저 종료\"\"\"\n        if self.driver:\n            self.driver.quit()\n            logger.info(\"브라우저 종료\")\n\nif __name__ == \"__main__\":\n    monitor = AttokRealTimeMonitor()\n    try:\n        monitor.login()\n        monitor.monitor()\n    finally:\n        monitor.close()"}}],"stop_reason":null,"stop_sequence":null,"usage":{"input_tokens":2,"cache_creation_input_tokens":2344,"cache_read_input_tokens":132674,"cache_creation":{"ephemeral_5m_input_tokens":2344,"ephemeral_1h_input_tokens":0},"output_tokens":1,"service_tier":"standard"}},"requestId":"req_011CSxKUHqM4ofwWCbQs6zVj","type":"assistant","uuid":"2b030e82-13bf-472c-a2ad-3dda44be7531","timestamp":"2025-09-09T06:02:57.377Z"}
