rrent_url = driver.current_url\n        logger.info(f\"현재 페이지: {current_url}\")\n        \n        # JavaScript로 모든 요소 분석\n        js_analysis = \"\"\"\n        var results = {\n            url: window.location.href,\n            title: document.title,\n            iframes: document.querySelectorAll('iframe').length,\n            tables: document.querySelectorAll('table').length,\n            divs_with_id: [],\n            colored_elements: [],\n            student_patterns: []\n        };\n        \n        // ID가 있는 모든 div\n        document.querySelectorAll('div[id]').forEach(function(div) {\n            results.divs_with_id.push({\n                id: div.id,\n                text: div.innerText ? div.innerText.substring(0, 50) : '',\n                bgColor: window.getComputedStyle(div).backgroundColor\n            });\n        });\n        \n        // 배경색이 있는 모든 요소\n        var allElements = document.querySelectorAll('*');\n        for (var i = 0; i < allElements.length; i++) {\n            var elem = allElements[i];\n            var bgColor = window.getComputedStyle(elem).backgroundColor;\n            var bgAttr = elem.getAttribute('bgcolor');\n            \n            if (bgColor && bgColor !== 'rgba(0, 0, 0, 0)' && bgColor !== 'transparent') {\n                if (elem.innerText && elem.innerText.length > 0) {\n                    results.colored_elements.push({\n                        tag: elem.tagName,\n                        id: elem.id,\n                        class: elem.className,\n                        bgColor: bgColor,\n                        bgAttr: bgAttr,\n                        text: elem.innerText.substring(0, 30)\n                    });\n                }\n            }\n        }\n        \n        // 학생으로 추정되는 패턴 찾기\n        var koreanNamePattern = /[가-힣]{2,4}/;\n        document.querySelectorAll('td, div, span').forEach(function(elem) {\n            var text = elem.innerText || '';\n            if (koreanNamePattern.test(text) && text.length < 20) {\n                var bgColor = window.getComputedStyle(elem).backgroundColor;\n                results.student_patterns.push({\n                    tag: elem.tagName,\n                    id: elem.id,\n                    text: text,\n                    bgColor: bgColor,\n                    parent: elem.parentElement ? elem.parentElement.tagName : null\n                });\n            }\n        });\n        \n        return results;\n        \"\"\"\n        \n        analysis_results = driver.execute_script(js_analysis)\n        \n        # 결과 출력\n        logger.info(f\"\\n=== 분석 결과 ===\")\n        logger.info(f\"URL: {analysis_results['url']}\")\n        logger.info(f\"제목: {analysis_results['title']}\")\n        logger.info(f\"iframe 수: {analysis_results['iframes']}\")\n        logger.info(f\"테이블 수: {analysis_results['tables']}\")\n        \n        logger.info(f\"\\nID가 있는 div: {len(analysis_results['divs_with_id'])}개\")\n        for div in analysis_results['divs_with_id'][:5]:\n            if 'std' in div['id'].lower() or '학' in div['text']:\n                logger.info(f\"  ⭐ {div}\")\n        \n        logger.info(f\"\\n배경색이 있는 요소: {len(analysis_results['colored_elements'])}개\")\n        for elem in analysis_results['colored_elements'][:10]:\n            if 'skyblue' in str(elem['bgColor']).lower() or '135, 206, 235' in str(elem['bgColor']):\n                logger.info(f\"  🔵 하늘색: {elem}\")\n        \n        logger.info(f\"\\n학생 패턴: {len(analysis_results['student_patterns'])}개\")\n        for pattern in analysis_results['student_patterns'][:10]:\n            logger.info(f\"  👤 {pattern}\")\n        \n        # 결과 저장\n        with open(\"attok_auto_analysis.json\", \"w\", encoding=\"utf-8\") as f:\n            json.dump(analysis_results, f, ensure_ascii=False, indent=2)\n        logger.info(\"\\n결과 저장: attok_auto_analysis.json\")\n        \n        # iframe 내부 확인\n        iframes = driver.find_elements(By.TAG_NAME, \"iframe\")\n        i